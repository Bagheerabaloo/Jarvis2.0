{
  "manifest_version": 2,
  "name": "Navigator Override FF",
  "version": "1.0",
  "description": "Override di navigator.webdriver nel main world.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
